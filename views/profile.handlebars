<div class="container-fluid">
  <h3> My Reviews:</h3>
  {{!-- {{#each review}} --}}
  {{#each reviewData as |review|}}
  <div class="card mb-3">
    <div class="card-body">
      <h5 class="card-title">{{review.attraction.name}}</h5>
      <p class="card-text">{{review.body}}</p>
      <button id="edit-post" type="button" class="btn btn-primary">Edit</button>
      <button id="delete-post" type="button" class="btn btn-primary">Delete</button>
    </div>
  </div>
  {{/each}}
</div>
<div class="container">
  <div class="card mb-3">
    <div class="card-body">
      <label for="edit-body" class="form-label">Edit Review</label>
      <textarea class="form-control" id="edit-body" rows="3"></textarea>
      <button id="submit" type="button" class="btn btn-primary">Save</button>
    </div>
  </div>
</div>